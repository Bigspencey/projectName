	location.href = theLink;
}
// The following snippet of code get the windows width for open popup windows (VIP Privacy Policy and TOU).
$(document).ready(function() {
	docWidth = $(window).width();
	if (!docWidth)
		docWidth = $(document).width();
	if (!docWidth)
		docWidth = 1000;
});

/*
See the session manager before modifying this function
*/
function registerConversionEvent(categoryID,eventID) {
	var cmConversionCookie = readCookie("CM_EVENT");
	//alert("Raw Cookie:" + cmConversionCookie);
	if(cmConversionCookie == null) {
		cmConversionTags = new Array();
	} else {
		cmConversionTags = eval('(' + cmConversionCookie + ')');
	}
	//alert("reg: START" + cmConversionTags.toString() );
	found = false;
	if(cmConversionTags instanceof Array) {
		for(i=0;i<cmConversionTags.length;i++) {
			if(cmConversionTags[i] == categoryID + "~" + eventID) {
				found = true;
				break;
			}
		}
		if(!found) {
			cmConversionTags.push(categoryID + "~" + eventID);
		}
		for(i=0;i<cmConversionTags.length;i++) {
			cmConversionTags[i]=  "\"" + cmConversionTags[i] + "\"";
		}
		createCookie("CM_EVENT","[" + cmConversionTags.toString() + "]");
	}
	//alert("reg: END" + cmConversionTags.toString() );
}
/*
See the session manager before modifying this function
*/
function completeConversionEvent(categoryID,eventID,deleteEvent) {
	var cmConversionCookie = readCookie("CM_EVENT");
	if(typeof(deleteEvent) == "undefined") {
		deleteEvent = true;
	}
	//alert("CompleteEvent: START" + cmConversionTags.toString() );
	if(cmConversionCookie == null) {
		cmConversionTags = new Array();
	} else {
		cmConversionTags = eval('(' + cmConversionCookie + ')');
	}
	found = false;
	finishArray = new Array();
	if(cmConversionTags instanceof Array) {
		for(i=0;i<cmConversionTags.length;i++) {
			if(cmConversionTags[i] == (categoryID + "~" + eventID)) {
				found = true;
				if(!deleteEvent) {
					finishArray.push("\"" + cmConversionTags[i] + "\"");
				}
			} else {
				finishArray.push("\"" + cmConversionTags[i] + "\"");
			}
		}
		//alert("CompleteEvent: END" + finishArray.toString() );
		createCookie("CM_EVENT","[" + finishArray.toString() + "]");
	}
	
	return found;
}

// coremetrics
function runCMConversionEventTag (data) {
	if(data.conversionEvent != "Continue Shopping"){
		cmCreateConversionEventTag(data.conversionEvent, 1, data.conversionCategory, 0);
	}
	registerConversionEvent(data.conversionCategory, data.conversionEvent);
}

/***
	Method:  add2cartJS()
	Input Attributes:  
	.	data . a JSON object representing the request details
		o	sku . the Product SKU to be added to cart
		o	size . the Product Size to be added to cart
		o	qty . the requested Quantity to be added to cart
		o	cm . (optional) the CoreMetrics Shop5 action tag . will default to .JSATC. 
		o	cm_pageId . (required) the CoreMetrics Page ID for Add To Cart Page View
		o	cm_categoryId . (required) the CoreMetrics Category ID for Add To Cart Page View 
	.	callback . the callback function will receive two arguments:  data . a JSON object defining the Add to Cart response; status . the status of the AJAX call
 ***/
function add2cartJS(data, callback) {
	try {
		if (!data.cm)
    			data.cm = 'JSATC';

		if (data.cm_pageId && data.cm_categoryId)
			cmCreatePageviewTag(data.cm_pageId, data.cm_categoryId);

		$.post(
			"/catalog/addToCartJSON.cfm"
		, 	data
		,	callback
		,	"json"
		);
	}
	catch (ex) {
		return ex;
	}

	return null;
}

/***
  This JavaScript Class provides methods:
      init(url) - initializes the client sessions by making an AJAX call to the server which is expected to return a JSON object that exposes relevant session properties
      initWith(callback) - pushes a callback method to be executed after the session is initialized
      onInit(data) - callback method after the AJAX call has been successfully completd
***/
function ClientSessionManager() {

        var callbacks = [];
        var session = {};

        this.init = function(sessionUrl) {
                $.getJSON(sessionUrl, {}, this.onInit);
        }

        this.onInit = function(data) {
        	session = data;
                for (var i=0,j=callbacks.length; i<j; i++) {
                        try {
                                callbacks[i](session);
                        }
                        catch(e) {
                                // alert(e);
                        }
                }
        };

        this.initWith = function(callback) {
                callbacks[callbacks.length] = callback;
        };
        
        this.getCustomerGreeting = function() {
        	return session.profile.greeting;
        }

        this.getCartSummary = function() {
        	var cart_summary = {};
        	cart_summary.item_count = session.cart.item_count;
        	cart_summary.sub_total = session.cart.sub_total;
        	return cart_summary;
        }
}

// Look up zip code and update city and state fields
function postalCodeLookup (zipObj, phoneObj, cityObj, stateObj, calledFrom) {
				
	function tranlatePostalCode(postalCode, callback) {
		// RewriteRule ^/zipcoder/(.*) http://ziptasticapi.com/$1 [P,QSA,NS]
		$.getJSON('/zipcoder/' + postalCode + '?cd=1d', callback)
			.fail(function(jqxhr, textStatus, error) {
				console.log("Request Failed: " + textStatus + ", " + error);
				if (calledFrom == "mobile_checkout") {
					addressPane.handlePostalCodeProcessingDialog('hide');
					cityObj.prop('disabled', false).focus().click();
				}
		});
	}
	
	function handlePostalCode(event) {
		if (zipObj.val().trim() != "") {
			if (calledFrom == "mobile_checkout") {
				hideKeyboardOnTouchDevice();
				cityObj.prop('disabled', true);
				addressPane.handlePostalCodeProcessingDialog('display');
			}
			tranlatePostalCode(zipObj.val(), function(data) {
				if (calledFrom == "mobile_checkout") {
					addressPane.handlePostalCodeProcessingDialog('hide');
					cityObj.prop('disabled', false);
				}
				if (data['city'] && data['state']) {
					stateObj.focus().val(data["state"]).blur();
					stateObj.trigger("change");
					if (data["city"].length < 16) {
						cityObj.focus().val(data["city"]).blur();
						cityObj.trigger("change");
						if (isTouchDevice())
							phoneObj.click();
						else
							phoneObj.focus();
					} else {
						if (isTouchDevice())
							cityObj.click();
						else
							cityObj.focus();
					}
				}
			});
		}
	}

    // unbind previously set event namespace
	zipObj.off('blur.postalCodeLookup');

	// bind new event namespace for postal code lookup
	zipObj.on('blur.postalCodeLookup', handlePostalCode);
}



function internationalPostalCodeLookup (countryCode, zipObj, phoneObj, cityObj, stateObj, calledFrom) {
	
	function tranlatePostalCode(postalCode, countryCode, callback) {
		// RewriteRule ^/zipcoder_v2/(.*)/(.*) http://zip.elevenbasetwo.com/v2/$1/$2 [P,QSA,NS]
		$.getJSON('/zipcoder_v2/' + countryCode + '/' + postalCode + '?cd=1d', callback)
			.fail(function(jqxhr, textStatus, error) {
				console.log("Request Failed: " + textStatus + ", " + error);
				if (calledFrom == "mobile_checkout") {
					addressPane.handlePostalCodeProcessingDialog('hide');
					cityObj.prop('disabled', false).focus().click();
				}
		});
	}
	
	function handlePostalCode(event) {
		if (zipObj.val().trim() != "") {
			if (calledFrom == "mobile_checkout") {
				hideKeyboardOnTouchDevice();
				cityObj.prop('disabled', true);
				addressPane.handlePostalCodeProcessingDialog('display');
			}
			tranlatePostalCode(zipObj.val(), countryCode, function(data) {
				if (calledFrom == "mobile_checkout") {
					addressPane.handlePostalCodeProcessingDialog('hide');
					cityObj.prop('disabled', false);
				}
				if (data['city'] && data['state']) {
					if (countryCode == "CA") {
						stateObj.focus().val($(stateObj).find("option:contains('" + data["state"] + "')").val()).blur();
					}
					if (data["city"].length < 16) {
						cityObj.focus().val(data["city"]).blur();
						phoneObj.focus();
					} else {
						cityObj.focus();
					}
				}
			});
		}
	}
	
	// unbind previously set event namespace
	zipObj.off('blur.postalCodeLookup');

	// bind new event namespace for postal code lookup
	zipObj.on('blur.postalCodeLookup', handlePostalCode);
}

function hideKeyboardOnTouchDevice () {
	if (isTouchDevice()) {
		document.activeElement.blur();
	}
}

function isUserAgent(val) {
	var userAgent = navigator.userAgent || navigator.vendor || window.opera;
	return userAgent.match(new RegExp(val, "i")) != null;
}

function isTouchDevice(){
	
	if(typeof(navigator.userAgent) != "undefined"){

		if(navigator.userAgent.match(/Mobile|iP(hone|od|ad)|Android|BlackBerry|IEMobile|Kindle|NetFront|Silk-Accelerated|(hpw|web)OS|Fennec|Minimo|Opera M(obi|ini)|Blazer|Dolfin|Dolphin|Skyfire|Zune/i) != null){
			return true;
		}
	}
	
	return false;
}



/*** 
	Content localization functions
***/

function parseCurrency(v, l) {
	switch(l) {
		case 'fr_CA':
			return parseFloat(v.replace(',','.').replace(/[^\d.]/g,''));
	}

	return parseFloat(v.replace(/[^\d.]/g,''));
}

function formatCurrency(v, l) {	
	var c = {
		prefix: "$"
	};

	switch(l) {
		case 'fr_CA':
			c = {
				suffix:" $"
			 ,	thousandsSeparator:" "
			 ,	decimalSeparator:","
			};			  
			break;
	}
	return numberFormat(v, c);
}

function numberFormat(v, c) {
	var config = {
		prefix: ""
	,	thousandsSeparator: ","
	,	decimalSeparator: "."
	,	decimalPlaces: 2
	,	suffix: ""			
	};

	$.extend(config, c);

	var number = parseFloat(v).toFixed(config.decimalPlaces);

	var isNegative = (number< 0);

	var index = number.lastIndexOf(".");
	index = (index > -1) ? index : number.length;

	var wholePart = number.substring(0, index);
	var decimalPart = number.substring(index+1, number.length);

	var newWhole = "";
	for (var i=wholePart.length, j=0; j<=i; j++) {
		newWhole = wholePart.substring(i-j, i-j+1) + newWhole;
		if (j != 0 && j%3 == 0 && j != (isNegative ? i - 1 : i)) {
			newWhole = config.thousandsSeparator + newWhole;
		}

	}

	number = config.prefix + newWhole + config.decimalSeparator + decimalPart + config.suffix;

	return number;
}

// Enter premier callout text within the DOM element passed
// Args:
//	elem - DOM element in which to enter callouts. Premier callout will replace any internal text or html.
//      Notes: - MUST have data-bogo defined as elem attribute! Value is the sourcecode.
//      i.e. <div data-bogo="#sourcecode#" class="premierCallout"></div>
//  callouts - array of callouts to show. Options: callout, icon, bubble. Will display in order passed in array.
//  tipOptions - options for Tipped object. Defaults are below. Anything passed in here will override defaults, if defined.
function populatePremierCallout(elem, callouts, tipOptions) {
    var co = $(elem);
    var tipBogoConfig = {};
    var sc = co.attr('data-bogo').trim();
    co.html('');
    if (typeof premierBogoDetails[sc] !== 'undefined') {
        if (typeof tipOptions === 'object') {$.extend(tipBogoConfig,tipOptions);}
        for (var i=0; i < callouts.length; i++) {
            if (callouts[i] === 'bubble') {
                var a = $('<a></a>');
                var aId = 'Info_' + Math.round(Math.random()*100000000).toString();
                a.addClass('info_icon').attr('id', aId);
                co.append(a);
                Tipped.create('#' + aId, premierBogoDetails[sc]['bubble'], tipBogoConfig);
                a.attr('title', premierBogoDetails[sc]['bubble']);
            } else {
                co.append(premierBogoDetails[sc][callouts[i]]);
            }
        }
    }
}

var geocoder;

function initializeGeoCoder() {
	geocoder = new google.maps.Geocoder();
}

function geocodeAddress(address, callback) {
	if(typeof geocoder == 'undefined'){
		initializeGeoCoder();
	}
    var region = locale.split('_')[1] === 'CA' ? 'ca' : 'us';
	geocoder.geocode({'address': address, 'region' : region}, function(results, status) {
		if (status == google.maps.GeocoderStatus.OK) {
			return callback(results[0], true);
		}
	
		return callback([], false);
	});
}

$(document).ready(function() {
	if (typeof google !== 'undefined') {
		try {
			initializeGeoCoder();
			google.maps.event.addDomListener(window, 'load', initializeGeoCoder);
		}
		catch (e) {
			console.log(e);
		}
	}
});

// Returns a function, that, as long as it continues to be invoked, will not
// be triggered. The function will be called after it stops being called for
// N milliseconds. If `immediate` is passed, trigger the function on the
// leading edge, instead of the trailing.
function debounce(func, wait, immediate) {
	var timeout;
	return function() {
		var context = this, args = arguments;
		var later = function() {
			timeout = null;
			if (!immediate) func.apply(context, args);
		};
		var callNow = immediate && !timeout;
		clearTimeout(timeout);
		timeout = setTimeout(later, wait);
		if (callNow) func.apply(context, args);
	};
};

function getCharSetRegEx(charSetType) {
	var charSetRegEx = "";

	if (typeof charSetType == 'undefined') {
		var charSetType = "default";
	}

	switch (charSetType) {
		case "ISO-8859-1":
			charSetRegEx = "[\x09\x0A\x0D\x20-\x7E\xA0-\xFF]";
			break;
		default:
			charSetRegEx = ".*";
	}

	return charSetRegEx;
}

function updateLoginStatus (context, authenticationState) {
	$.ajax({
		type: "GET", 
		url: "/getWelcomeGreeting", 
		async: true, 
		success: function(responseText) {
			var customerGreeting = responseText;
			$(document).trigger("login_status_changed", [context, authenticationState, customerGreeting]);
		}
	});
}
function postsizesearch(frm, prod_tp){
	ajax = getHTTPObject();
	var actionURL = sizeSearchActionURL;
	var frmValues = getFormValues(frm);
   	postAjaxForm(frmValues, actionURL, function(){update_size_search(document.getElementById(prod_tp + '_size_search'))});
}
function getsizesearch(actionURL, prod_tp){
	ajax = getHTTPObject();
   	loadXMLDoc(actionURL, function(){update_size_search(document.getElementById(prod_tp + '_size_search'))});
}
function update_size_search(cont) {
	var container = new Object();
	container = cont;
	switch (ajax.readyState) {
		case 1:
			break;
		case 2:
			break;
		case 3:
			break;				
		case 4:
   			if (ajax.status != 200) return;
			insertFragment(container,getFieldFromContent("content",ajax.responseText));
	}
}

